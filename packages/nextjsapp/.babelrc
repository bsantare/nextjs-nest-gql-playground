{
  "presets": [
    "next/babel"
  ],
  "plugins": [
    [
      "styled-components",
      {
        "ssr": true,
        "displayName": true,
        "preprocess": false
      }
    ],
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@pages": "./src/pages",
          "@components": "./src/components",
          "@libs": "./src/lib",
          "@styles": "./src/styles"
        }
      }
    ]
  ]
}
